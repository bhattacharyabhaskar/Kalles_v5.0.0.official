{%- comment -%}{{ 'hdt-card-products.css' | asset_url | stylesheet_tag }}{%- endcomment -%}

{{ 'hdt-main-pages.css' | asset_url | stylesheet_tag }}
{{ 'facets.css' | asset_url | stylesheet_tag }}
{{ 'hdt-widget.css' | asset_url | stylesheet_tag }}
{%- liquid
  echo 'hdt-pagination.css' | asset_url | stylesheet_tag
  assign color_list = 'products.theme_settings.color' | t | downcase | strip | replace: ' ,', ',' | replace: ', ', ',' | split: ',' | uniq
  assign size_list = 'products.theme_settings.size' | t | downcase | strip | replace: ' ,', ',' | replace: ', ', ',' | split: ',' | uniq
  assign products_id = 'products-' | append: section.id
  assign arr_col_from_attr = cart.attributes.products_items_per_row | split: '.'
  assign col_mb = section.settings.col_mb | plus: 0
  assign col_tb = section.settings.col_tb | plus: 0
  assign col_dk = section.settings.col_dk | plus: 0
  assign col_mobile = arr_col_from_attr[0] | default: col_mb | plus: 0
  assign col_tablet = arr_col_from_attr[1] | default: col_tb | plus: 0
  assign col_tablet = col_tablet | at_most: col_dk | plus: 0
  assign col_desktop = arr_col_from_attr[2] | default: col_dk | plus: 0
  if section.blocks.size > 0
    assign has_sidebar = true
    case section.settings.sidebar_size
      when 'small'
        assign width_sidebar = '20%'
      when 'medium'
        assign width_sidebar = '25%'
      else
      assign width_sidebar = '33.3333333%'
    endcase
  else
    assign has_sidebar = false
  endif
  assign blocks_type = section.blocks | map: "type"
-%}
{%- liquid
  assign current_width = settings.page_width | append: 'px'
  assign current_width_tb = '100vw'
  assign current_width_mb = '100vw'
  if section.settings.section_layout == "full_width"
      assign current_width = '100vw - 100px'
      assign current_width_tb = '100vw - 20px'
      assign current_width_mb = '100vw - 10px'
  endif
  if section.settings.space_items == "x"
    assign space_item = settings.spacing_grid_horizontal
    assign space_item_tb = settings.spacing_grid_horizontal | divided_by: 2
    assign space_item_mb = settings.spacing_grid_horizontal | divided_by: 2
  else
    assign space_item = section.settings.space_items
    assign space_item_tb = space_item | times: 0.666 | round
    assign space_item_mb = space_item | times: 0.333 | round
  endif


  case section.settings.section_layout 
    when 'container'
      assign parent = 'hdt-container'
      assign child  = 'hdt-section-spacing hdt-s-gradient hdt-relative'
    when 'wrapper_container'
      assign parent = 'hdt-section-spacing hdt-s-gradient hdt-relative'
      assign child  = 'hdt-container'
    when 'full_width'
      assign parent = 'hdt-container-full hdt-section-spacing hdt-s-gradient hdt-relative'
      assign child  = ''
      assign sizes  = '100vw'
  endcase
-%}

{%- capture sizes -%}
  (min-width: 1150px) calc(({{ current_width }} -  {{ space_item }} * {{ section.settings.col_dk | minus: 1 }}) * {{ 1.00 | divided_by: section.settings.col_dk | round: 2 }}) {{- -}}
  , (min-width: 768px) calc( ({{ current_width_tb }} - {{ space_item_tb }} * {{ section.settings.col_tb | minus: 1 }} ) * {{ 1.00 | divided_by: section.settings.col_tb | round: 2 }})  {{- -}}
  , calc( ({{ current_width_mb }} - {{ space_item_mb }} * {{ section.settings.col_mb | minus: 1 }} ) * {{ 1.00 | divided_by: section.settings.col_mb | round: 2 }})
{%- endcapture -%}

{%- style -%}
  #shopify-section-{{ section.id }}{
    --section-pt: {{ section.settings.padding_top }}px;
    --section-pb: {{ section.settings.padding_bottom }}px; 
    --section-prate: {{ section.settings.padding_rate }};
  }
  #{{ section.id }} .hdt-row-grid {
    --spacing-x: {{ space_item_mb }}px;
    --spacing-y: {{ space_item }}px;
  } 
  @media(min-width: 768px){
    #{{ section.id }} .hdt-row-grid {
      --spacing-x: {{ space_item_tb }}px;
    } 
  }
  @media(min-width: 1150px){
    #{{ section.id }} .hdt-row-grid {
      --spacing-x: {{ space_item }}px;
    } 
  }
  {%- if has_sidebar -%}
    @media(min-width:1150px){
      .hdt-row-flex .hdt-shop-content {
        width: 100%;
      }
      .hdt-row-flex.lg\:hdt-flex-cols-2 .hdt-shop-content {
        width: calc(100% - {{ width_sidebar }} - var(--spacing-x)/2);
      }
      .hdt-row-flex.lg\:hdt-flex-cols-2 .hdt-main-sidebar {
        width:calc({{ width_sidebar }} - var(--spacing-x)/2);
      }
      
      /* .hdt-collection-products.lg\:hdt-grid-cols-4 .hdt-card-product:not(.hdt-pr-style2):not(.hdt-pr-style3):not(.hdt-pr-style4):not(.hdt-pr-style6):not(.hdt-pr-style7):not(.hdt-pr-style10):not(.hdt-pr-style11) .hdt-product-btns .hdt-pr_btn,
      .hdt-collection-products.lg\:hdt-grid-cols-4 .hdt-card-product .hdt-product-btns .hdt-pr_btn,.hdt-collection-products.lg\:hdt-grid-cols-4 .hdt-pr-style2 .hdt-card-product__media .hdt-product-btns .hdt-quantity-wrapp + .hdt-pr_btn {
        width:40px;
        min-width:40px;
        height:40px;
      } */
    }
    {%- if section.settings.sidebar_drawer == false -%}
      .hdt-row-flex .hdt-main-sidebar {
        width: calc({{ width_sidebar }} - var(--spacing-x) / 2);
      }
      .hdt-view-layout-switch button.hdt-view-grid5,
      .hdt-view-layout-switch button.hdt-view-grid6 {
        display:none
      }
      .lg\:hdt-grid-cols-5,
      .lg\:hdt-grid-cols-6 {
        --cols: 4;
      }
    {%- endif -%}
  {%- endif -%}
{%- endstyle -%}
{%- capture sizes -%}
 {%- render 'sizes', enable_list: true, full_width: section.settings.section_layout == "full_width", col_mobile: col_mobile, col_tablet: col_tablet, col_desktop: col_desktop -%}
{%- endcapture -%}
{%- if section.settings.show_description and collection.description != blank -%}
  {%- capture collection_description -%}
    <div class="{{ parent }} hdt-main-collection-description" {%- if section.settings.colors_by_section %} color-scheme="{{ section.settings.color_scheme }}" style="--background-opacity: {{ section.settings.background_opacity }};"{%- endif %}>
      <div class="{{ child }}">
        <div class="hdt-rte">
          {{ collection.description }}
        </div>
      </div>
    </div>
  {%- endcapture -%}
{%- endif -%}
<div id="{{ section.id }}" main-content class="hdt-main-collection-content hdt-section-spacing">
  {%- if section.settings.description_position == 'before' -%}
    {{ collection_description }}
  {%- endif -%}
  <div class="{{ parent }}" {%- if section.settings.colors_by_section %} color-scheme="{{ section.settings.color_scheme }}" style="--background-opacity: {{ section.settings.background_opacity }};"{%- endif %}>
    <div class="{{ child }}">
      {%- if section.settings.section_background != blank -%}
        {% liquid
          assign image = section.settings.section_background
        %}
          {{ image | image_url: width: image.width | image_tag: width: image.width, height: image.height, sizes: sizes, widths: '200,300,400,500,600,700,800,900,1000,1100,1200,1300,1400,1500,1600,1700,1800,1900,2000', loading: 'lazy', class: 'hdt-section-background hdt-absolute hdt-top-0 hdt-left-0 hdt-w-full hdt-h-full hdt-pe-none hdt-object-cover hdt-object-center' }}
      {%- endif -%}
      {%- if collection.all_products_count > 0 -%}
      <div class="hdt-shop-control hdt-justify-between hdt-align-center hdt-row-flex hdt-flex-cols-3">
        {%- comment -%} Filter {%- endcomment -%}
          <div class="hdt-col hdt-control-filter">
            
            {%- if section.settings.enable_filtering -%}
              <button class="hdt-filter_btn hdt-inline-flex hdt-align-center {% if has_sidebar and blocks_type contains 'filter' -%} lg:hdt-hidden{%- endif -%}" aria-controls="hdt-filter_form">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512" width="16" height="16"><path d="M324.4 64C339.6 64 352 76.37 352 91.63C352 98.32 349.6 104.8 345.2 109.8L240 230V423.6C240 437.1 229.1 448 215.6 448C210.3 448 205.2 446.3 200.9 443.1L124.7 385.6C116.7 379.5 112 370.1 112 360V230L6.836 109.8C2.429 104.8 0 98.32 0 91.63C0 76.37 12.37 64 27.63 64H324.4zM144 224V360L208 408.3V223.1C208 220.1 209.4 216.4 211.1 213.5L314.7 95.1H37.26L140 213.5C142.6 216.4 143.1 220.1 143.1 223.1L144 224zM496 400C504.8 400 512 407.2 512 416C512 424.8 504.8 432 496 432H336C327.2 432 320 424.8 320 416C320 407.2 327.2 400 336 400H496zM320 256C320 247.2 327.2 240 336 240H496C504.8 240 512 247.2 512 256C512 264.8 504.8 272 496 272H336C327.2 272 320 264.8 320 256zM496 80C504.8 80 512 87.16 512 96C512 104.8 504.8 112 496 112H400C391.2 112 384 104.8 384 96C384 87.16 391.2 80 400 80H496z"></path></svg>
                <span>{{ 'products.facets.filter_button' | t -}}</span>
              </button>
              {%- render 'facets', results: collection, section_id: section.id, enable_render_on_change: true -%}
            {%- endif -%}
          
          </div>
        {%- comment -%} endFilter {%- endcomment -%}
        <div class="hdt-col hdt-control-layout">
          {%- if section.settings.enable_num_cols_selector -%}
            {% liquid
              if section.settings.sidebar_drawer == false
                assign col_desktop = arr_col_from_attr[2] | default: col_dk | plus: 0 | at_most: 4
              endif
            %}
            {%- render 'layout-switch', results: collection, section_id: section.id, controls_id: products_id, col_mobile: col_mobile, col_tablet: col_tablet, col_desktop: col_desktop -%}
          {%- endif -%}
        </div>
        <div class="hdt-col hdt-control-sorting">
          {%- if section.settings.enable_sorting -%}
            {%- render 'sort-by-popover', results: collection, section_id: section.id -%}
          {%- endif -%}
        </div>
      </div>
      {%- endif -%}
      {%- render 'active-filters', results: collection, results_count: collection.products_count -%}
      <div class="{% if has_sidebar and section.settings.sidebar_drawer == false %} hdt-row-flex hdt-sidebar_drawer_{{ section.settings.sidebar_drawer }} hdt-flex-cols-1 lg:hdt-flex-cols-2{% else %} hdt-flex-cols-1{% endif %} ">
        <div class="hdt-shop-content hdt-col">
          {%- if collection.all_products_count == 0 -%}
            <div class="hdt-template hdt-template--empty hdt-text-center hdt-flex hdt-flex-col hdt-align-center hdt-justify-center">
              <p class="hdt-h5 hdt-s-text">{{ 'sections.collection_template.empty' | t -}}</p>
              <a class="hdt-btn hdt-btn hdt-oh hdt-inline-flex hdt-align-center hdt-justify-center hdt-relative hdt-btn-solid hdt-font-medium" href="{{ routes.all_products_collection_url }}" hdt-reveal="slide-in" timeline>
                {{ 'sections.collection_template.continue_shopping' | t }}
              </a>
            </div>
          {%- else -%}
            {%- paginate collection.products by section.settings.products_count -%}
              {%- if collection.products.size == 0 -%}
                {%- comment -%} no found {%- endcomment -%}
                <div class="hdt-template hdt-template--filtered hdt-template--empty hdt-text-center hdt-flex hdt-flex-col hdt-align-center hdt-justify-center">
                  <p class="hdt-h5 hdt-s-text">{{ 'products.facets.no_found' | t -}}</p>
                  {{ 'products.facets.use_fewer_filters_html' | t: link: collection.url, class: '' }}
                </div>
              {%- else -%}
                <hdt-reval-items reveal-on-scroll="true" id="{{ products_id }}" scroll-into-view class="hdt-collection-products hdt-collection-has-pr{{ section.settings.product_des }} hdt-pr-border_{{ section.settings.pr_border }} hdt-row-grid hdt-grid-cols-{{ col_mobile }} md:hdt-grid-cols-{{ col_tablet }} lg:hdt-grid-cols-{{ col_desktop }} hdt-ratio--{{ section.settings.image_ratio }}{% unless section.settings.image_size %} hdt-ratio--contain{% endunless %}">
                  {%- liquid
                    capture btn_wishlist_code
                      if settings.wishlist_mode != '0'
                        render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'right'
                      endif
                    endcapture
                    if col_mobile == 0 or col_tablet == 0 or col_desktop == 0
                      for product in collection.products
                        unless settings.remove_collection_url
                          assign product_url = product.url | within: collection
                        endunless
                        render 'card-product-list', card_product: product, class: "hdt-pr-list", section_id: section.id, sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                      endfor
                    else
                      case section.settings.product_des
                        when '1'
                          capture btn_wishlist_code
                            if settings.wishlist_mode != '0'
                              render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'right'
                            endif
                          endcapture
                          for product in collection.products
                            unless settings.remove_collection_url
                              assign product_url = product.url | within: collection
                            endunless
                            render 'card-product1', card_product: product, class: "hdt-pr-style1", section_id: section.id, sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                          endfor
                        when '2'
                          capture btn_wishlist_code
                            if settings.wishlist_mode != '0'
                              render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'top'
                            endif
                          endcapture
                          for product in collection.products
                            unless settings.remove_collection_url
                              assign product_url = product.url | within: collection
                            endunless
                            render 'card-product2', card_product: product, section_id: section.id, class: "hdt-pr-style2", sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                          endfor
                        when '3'
                          capture btn_wishlist_code
                            if settings.wishlist_mode != '0'
                              render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'left'
                            endif
                          endcapture
                          for product in collection.products
                            unless settings.remove_collection_url
                              assign product_url = product.url | within: collection
                            endunless
                            render 'card-product3', card_product: product, section_id: section.id, class: "hdt-pr-style3", sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                          endfor
                        when '4'
                          capture btn_wishlist_code
                            if settings.wishlist_mode != '0'
                              render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'top'
                            endif
                          endcapture
                          for product in collection.products
                            unless settings.remove_collection_url
                              assign product_url = product.url | within: collection
                            endunless
                            render 'card-product4', card_product: product, section_id: section.id, class: "hdt-pr-style4", sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                          endfor
                        when '5'
                          capture btn_wishlist_code
                            if settings.wishlist_mode != '0'
                              render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'left'
                            endif
                          endcapture
                          for product in collection.products
                            unless settings.remove_collection_url
                              assign product_url = product.url | within: collection
                            endunless
                            render 'card-product5', card_product: product, section_id: section.id, class: "hdt-pr-style5", sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                          endfor
                        when '6'
                          capture btn_wishlist_code
                            if settings.wishlist_mode != '0'
                              render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'left'
                            endif
                          endcapture
                          for product in collection.products
                            unless settings.remove_collection_url
                              assign product_url = product.url | within: collection
                            endunless
                            render 'card-product6', card_product: product, section_id: section.id, class: "hdt-pr-style6", sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                          endfor
                        when '7'
                          capture btn_wishlist_code
                            if settings.wishlist_mode != '0'
                              render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'left'
                            endif
                          endcapture
                          for product in collection.products
                            unless settings.remove_collection_url
                              assign product_url = product.url | within: collection
                            endunless
                            render 'card-product7', card_product: product, section_id: section.id, class: "hdt-pr-style7", sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                          endfor
                        when '8'
                          capture btn_wishlist_code
                            if settings.wishlist_mode != '0'
                              render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'left'
                            endif
                          endcapture
                          for product in collection.products
                            unless settings.remove_collection_url
                              assign product_url = product.url | within: collection
                            endunless
                            render 'card-product8', card_product: product, section_id: section.id, class: "hdt-pr-style8", sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                          endfor
                        when '9'
                          capture btn_wishlist_code
                            if settings.wishlist_mode != '0'
                              render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'right'
                            endif
                          endcapture
                          for product in collection.products
                            unless settings.remove_collection_url
                              assign product_url = product.url | within: collection
                            endunless
                            render 'card-product9', card_product: product, section_id: section.id, class: "hdt-pr-style9", sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                          endfor
                        when '10'
                          capture btn_wishlist_code
                            if settings.wishlist_mode != '0'
                              render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'right'
                            endif
                          endcapture
                          for product in collection.products
                            unless settings.remove_collection_url
                              assign product_url = product.url | within: collection
                            endunless
                            render 'card-product10', card_product: product, section_id: section.id, class: "hdt-pr-style10", sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                          endfor
                      endcase
                    endif
                  -%}
                </hdt-reval-items>
                {%- if paginate.pages > 1 -%}
                  {%- render 'pagination', paginate: paginate, anchor: '', section_id: section.id , pagination_type: section.settings.pagination_type, append_id: products_id, paginate_pos: section.settings.paginate_pos, dis_pagination: section.settings.dis_pagination, lm_text: section.settings.btn_label -%}
                {%- endif -%}
              {%- endif -%}
            {%- endpaginate -%}
          {%- endif -%}
        </div>
        {%- if has_sidebar -%}
          {% capture sidebar_content %}
            {% for block in section.blocks %}
              {%- case block.type -%}
                {%- when 'categories' -%}
                  <hdt-accordion class="hdt-contents">
                    <details class="hdt-widget hdt-widget-categories" open {{ block.shopify_attributes }}>
                      {%- if block.settings.widget_heading != blank -%}
                        <summary class="hdt-widget-title hdt-flex hdt-align-center hdt-justify-between hdt-text-xl lg:hdt-text-2xl hdt-font-normal">
                          <span>{{ block.settings.widget_heading }}</span>
                          <span accordion-icon>
                            <svg xmlns="http://www.w3.org/2000/svg" width="11" height="7" viewBox="0 0 11 7" fill="none"><path d="M11 5.5L5.5 6.55868e-08L-6.55868e-08 5.5L0.97625 6.47625L5.5 1.9525L10.0238 6.47625L11 5.5Z" fill="currentColor"/></svg>
                          </span>
                        </summary>
                      {%- endif -%}
                      <div class="hdt-widget-content hdt-current-scrollbar">
                        <div class="hdt-categories-list hdt-text-base">
                          {%- case block.settings.cat_source -%}
                            {%- when '1' -%}
                              {%- if collections.size > 0 -%}
                                {%- for category in collections -%}
                                    {%- if category.products.size > 0 -%}
                                      <div class="hdt-cate-item hdt-relative{%- if collection.handle == category.handle %} hdt-current-cate{%- endif -%}"><a href="{{ category.url }}">{{ category.title }}
                                      {% if block.settings.count %}
                                        <span class="hdt-count">({{ category.all_products_count }})</span>
                                      {%- endif -%}</a>
                                      </div>
                                    {%- endif -%}
                                {%- endfor -%}
                              {%- endif -%}
                            {%- else -%}
                              {%- for link in linklists[block.settings.cat_link_list].links -%}
                                {%- if link.type == 'collection_link' or link.type == 'catalog_link' -%}
                                  {%- if link.links != blank -%}
                                    <hdt-accordion class="hdt-contents" only-click-icon>
                                      <details class="hdt-pr hdt-menu_nested hdt-cate-item hdt-relative hdt-relative">
                                        <summary id="hdt-menu-{{ link.handle }}" data-url="{{ link.url }}" class="has_cats_toggle hdt-flex hdt-align-center hdt-justify-between hdt-relative">
                                          <span class="hdt-link_sm hdt-relative{%- if link.active %} hdt-current-cate{%- endif -%}">
                                            {{ link.title }} {%- if block.settings.count == true -%} <span class="hdt-count">({{ link.object.all_products_count | default: shop.products_count }})</span>{%- endif -%}
                                          </span>
                                          {% if link.links != blank %}
                                            <span class="hdt-btn-open-sub hdt-relative" accordion-icon></span>
                                          {% endif %}
                                        </summary>
                                        <div class="hdt-sub-menu hdt-children">
                                          {%- for child_link in link.links -%}
                                            {%- if child_link.type == 'collection_link' or child_link.type == 'catalog_link' -%}
                                              {%- if child_link.links != blank -%}
                                              <hdt-accordion class="hdt-contents" only-click-icon>
                                                <details class="hdt-pr hdt-menu_nested2 hdt-cate-item hdt-relative">
                                                  <summary id="hdt-menu-{{ child_link.handle }}" data-url="{{ child_link.url }}" class="hdt-cat-count hdt-flex hdt-align-center hdt-justify-between hdt-relative">
                                                    <span class="hdt-link_sm hdt-relative{%- if child_link.active %} hdt-current-cate{%- endif -%}">
                                                      {{ child_link.title }}
                                                    {%- if block.settings.count == true -%}
                                                      <span class="hdt-count">({{ child_link.object.all_products_count | default: shop.products_count }})</span>
                                                    {%- endif -%}
                                                    </span>
                                                    {% if link.links != blank %}
                                                      <span class="hdt-btn-open-sub hdt-relative" accordion-icon></span>
                                                    {% endif %}
                                                  </summary>
                                                  <div class="hdt-sub-menu hdt-children">
                                                    {%- for third_link in child_link.links -%}
                                                      {%- if third_link.type == 'collection_link' or third_link.type == 'catalog_link' -%}
                                                        <a href="{{ third_link.url }}" class="hdt-link_sm hdt-relative{%- if third_link.active %} hdt-current-cate{%- endif -%}">{{ third_link.title }}
                                                          {%- if block.settings.count == true -%}
                                                            <span class="hdt-count">({{ third_link.object.all_products_count | default: shop.products_count }})</span>
                                                          {%- endif -%}
                                                        </a>
                                                      {%- endif -%}
                                                    {%- endfor -%}
                                                  </div>
                                                </details>
                                              </hdt-accordion>
                                              {%- else -%}
                                                <div class="hdt-cate-item hdt-relative{%- if child_link.active %} hdt-current-cate{%- endif -%}">
                                                  <a href="{{ child_link.url }}">{{ child_link.title }} {%- if block.settings.count == true -%} <span class="hdt-count">({{ child_link.object.all_products_count | default: shop.products_count }})</span> {%- endif -%}</a>
                                                </div>
                                              {%- endif -%}
                                            {%- endif -%}
                                          {%- endfor -%}
                                        </div>
                                      </details>
                                    </hdt-accordion>
                                  {%- else -%}
                                    <div class="hdt-cate-item hdt-relative{%- if link.active %} hdt-current-cate{%- endif -%}"><a href="{{ link.url }}">{{ link.title }} {%- if block.settings.count == true -%} <span class="hdt-count">({{ link.object.all_products_count | default: shop.products_count }})</span>{%- endif -%}</a></div>
                                  {%- endif -%}
                                {%- endif -%}
                              {%- endfor -%}
                          {%- endcase -%}
                        </div>
                      </div>
                    </details>
                  </hdt-accordion>
                {%- when 'gallery' -%}
                  {%- liquid
                    assign open_link = block.settings.open_link
                    assign ARRhtml1 = 'a,,' | split: ','
                    assign ARRhtml2 = 'div,data-' | split: ','
                  -%}
                  <hdt-accordion class="hdt-contents">
                  <details class="hdt-widget hdt-widget-gallery" open {{ block.shopify_attributes }}>
                    {%- if block.settings.widget_heading != blank -%}
                      <summary class="hdt-widget-title hdt-flex hdt-align-center hdt-justify-between hdt-text-xl lg:hdt-text-2xl hdt-font-normal">
                        <span>{{ block.settings.widget_heading }}</span>
                        <span accordion-icon>
                          <svg xmlns="http://www.w3.org/2000/svg" width="11" height="7" viewBox="0 0 11 7" fill="none"><path d="M11 5.5L5.5 6.55868e-08L-6.55868e-08 5.5L0.97625 6.47625L5.5 1.9525L10.0238 6.47625L11 5.5Z" fill="currentColor"/></svg>
                        </span>
                      </summary>
                    {%- endif -%}
                    <div class="hdt-widget-content">
                      <div class="hdt-gallery-list hdt-row-grid hdt-justify-center hdt-ratio--square hdt-object-cover hdt-grid-cols-3">
                        {%- capture sizes -%}
                          (100% - 8px) * 1 / 3)
                        {%- endcapture -%}
                        {%- for i in (1..9) -%}
                          {%- liquid
                            assign image_list = 'img' | append: i
                            assign image_url_list = 'link' | append: i
                            assign url = block.settings[image_url_list]
                            assign image = block.settings[image_list]
                            if url == blank
                                assign ARRhtml = ARRhtml2
                            else
                                assign ARRhtml = ARRhtml1
                            endif
                          -%}
                          {%- if image != blank -%}
                            <div class="hdt-col-item hdt-gallery-item">
                              <{{ ARRhtml[0] }} {{ ARRhtml[1] }}href="{{ url }}" {{ ARRhtml[2] }}target="{{ open_link }}" class="hdt-effect hdt-relative hdt-oh hdt-d-block">
                                <div class="hdt-ratio" style="background: url({{ image | image_url: width: 1 }});">
                                  {{ image | image_url: width: image.width | image_tag: loading: 'lazy', width: image.width, height: image.height, class: '' , sizes: sizes, widths: "110,150,200,300,400,500,600" }}
                                </div>
                              </{{ ARRhtml[0] }}>
                            </div>
                          {%- endif -%}
                        {%- endfor -%}
                      </div>
                    </div>
                  </details>
                  </hdt-accordion>
                {%- when 'featured_products' -%}
                <hdt-accordion class="hdt-contents">
                  <details class="hdt-widget hdt-widget-featured-products" open {{ block.shopify_attributes }}>
                    {%- if block.settings.widget_heading != blank -%}
                      <summary class="hdt-widget-title hdt-flex hdt-align-center hdt-justify-between hdt-text-xl lg:hdt-text-2xl hdt-font-normal">
                        <span>{{ block.settings.widget_heading }}</span>
                        <span accordion-icon>
                          <svg class="hdt-facet-title_icon" xmlns="http://www.w3.org/2000/svg" width="11" height="7" viewBox="0 0 11 7" fill="none"><path d="M11 5.5L5.5 6.55868e-08L-6.55868e-08 5.5L0.97625 6.47625L5.5 1.9525L10.0238 6.47625L11 5.5Z" fill="currentColor"/></svg>
                        </span>
                      </summary>
                    {%- endif -%}
                    <div class="hdt-widget-content">
                      <div class="hdt-products-list hdt-row-grid hdt-row-grid--modify hdt-ratio--{{ block.settings.image_ratio }} {% if block.settings.image_size %} hdt-object-cover{% else %} hdt-object-contain{% endif %} lg:hdt-grid-cols-1">
                        {%- liquid
                          assign product_collection = block.settings.product_list | default: block.settings.collection.products
                          assign color_list = 'products.theme_settings.color' | t | downcase | strip | replace: ' ,', ',' | replace: ', ', ',' | split: ',' | uniq
                          assign size_list = 'products.theme_settings.size' | t | downcase | strip | replace: ' ,', ',' | replace: ', ', ',' | split: ',' | uniq
                        -%}
                          {%- if product_collection != blank -%}
                            {%- liquid
                            for product in product_collection limit: block.settings.products_count
                              render 'pr_loop_item', section_id: block.id, product: product, sizes: "92px", widths: "92, 100, 180, 200"
                            endfor -%}
                          {%- else -%}
                            {%- capture current -%}
                              {%- cycle 1, 2 , 3, 4 -%}
                            {%- endcapture -%}
                            {%- for i in (1..block.settings.products_count) -%}
                              <div class="hdt-card-product hdt-pr-loop hdt-loop-item">
                                <div class="hdt-card-product__wrapper hdt-flex hdt-loop-inner">
                                  <div class="hdt-card-product__media hdt-loop-img hdt-relative hdt-oh">
                                      <div class="hdt-product-img hdt-ratio" style="--aspect-ratioapt: 1">
                                        {{ 'product-apparel-' | append: current | placeholder_svg_tag: 'hdt-placeholder-svg' }}
                                      </div>
                                    <a data-pr-href class="hdt-full-width-link"  aria-label="link" href="{{ product.url }}"></a>
                                  </div>
                                  <div class="hdt-card-product__info">
                                    <a href="{{ product_url }}" class="hdt-card-product__title hdt-h6" data-not-instant>{{ 'onboarding.product_title' | t }}</a>
                                    {%- render 'card-price', product: product, price_class: '', show_compare_at_price: true -%}
                                  </div>
                                </div>
                              </div>
                            {%- endfor -%}
                          {%- endif -%}
                      </div>
                    </div>
                  </details>
                  </hdt-accordion>
                {%- when 'iconbox' -%}
                <hdt-accordion class="hdt-contents">
                  <details class="hdt-widget hdt-widget-iconbox" open {{ block.shopify_attributes }}>
                    {%- if block.settings.widget_heading != blank -%}
                      <summary class="hdt-widget-title hdt-flex hdt-align-center hdt-justify-between hdt-text-xl lg:hdt-text-2xl hdt-font-normal">
                        <span>{{ block.settings.widget_heading }}</span>
                        <span accordion-icon>
                          <svg xmlns="http://www.w3.org/2000/svg" width="11" height="7" viewBox="0 0 11 7" fill="none"><path d="M11 5.5L5.5 6.55868e-08L-6.55868e-08 5.5L0.97625 6.47625L5.5 1.9525L10.0238 6.47625L11 5.5Z" fill="currentColor"/></svg>
                        </span>
                      </summary>
                    {%- endif -%}
                    <div class="hdt-widget-content">
                      <div class="hdt-iconbox-list hdt-row-grid hdt-ratio--square hdt-object-cover lg:hdt-grid-cols-1">
                        {%- for i in (1..4) -%}
                          {%- liquid
                            assign icon_svg = 'icon_svg' | append: i
                            assign custom_svg = 'custom_svg' | append: i
                            assign iconbox_icon_img = 'iconbox_icon_img' | append: i
                            assign iconbox_content = 'iconbox_content' | append: i
                            assign link_item = 'link_item' | append: i
                            assign icon_svg = block.settings[icon_svg]
                            assign custom_svg = block.settings[custom_svg]
                            assign iconbox_icon_img = block.settings[iconbox_icon_img]
                            assign iconbox_content = block.settings[iconbox_content]
                            assign link_item = block.settings[link_item] -%}
                          {%- if iconbox_content != blank -%}
                            {%- assign iconbox_content = iconbox_content | replace: ' _', '_' | replace: '_ ', '_' | split: '_' -%}
                            <div class="hdt-iconbox-item hdt-flex hdt-align-center hdt-relative">
                              {% if link_item != blank %}<a data-pr-href class="hdt-full-width-link"  aria-label="link" href="{{ link_item }}"></a>{% endif %}
                              <span class="hdt-iconbox-icon hdt-flex hdt-align-center hdt-justify-center">
                                {%- if iconbox_icon_img != blank -%}
                                  {{ iconbox_icon_img | image_url: width: iconbox_icon_img.width | image_tag: loading: 'lazy', width: iconbox_icon_img.width, height: iconbox_icon_img.height, class: '' , sizes: sizes, widths: "24, 52, 48, 104, 120, 240" }}
                                {%- elsif custom_svg != blank  -%}
                                  {{ custom_svg }}
                                {%- else -%}
                                  {%- render 'icon_shipping', icon_name: icon_svg -%}
                                {%- endif -%}
                              </span>
                              <div class="hdt-iconbox-content">
                                <h4 class="hdt-sidebar-iconbox-title hdt-text-lg hdt-font-semibold">{{ iconbox_content[0] }}</h4>
                                <p class="hdt-sidebar-iconbox-desc hdt-text-base hdt-font-normal hdt-s-text2">{{ iconbox_content[1] }}</p>
                              </div>
                            </div>
                          {%- endif -%}
                      {%- endfor -%}
                      </div>
                    </div>
                  </details>
                  </hdt-accordion>
                {%- when 'html' -%}
                <hdt-accordion class="hdt-contents">
                  <details class="hdt-widget hdt-widget-html" open {{ block.shopify_attributes }}>
                    {%- if block.settings.widget_heading != blank -%}
                      <summary class="hdt-widget-title hdt-flex hdt-align-center hdt-justify-between hdt-text-xl lg:hdt-text-2xl hdt-font-normal">
                        <span>{{ block.settings.widget_heading }}</span>
                        <span accordion-icon>
                          <svg xmlns="http://www.w3.org/2000/svg" width="11" height="7" viewBox="0 0 11 7" fill="none"><path d="M11 5.5L5.5 6.55868e-08L-6.55868e-08 5.5L0.97625 6.47625L5.5 1.9525L10.0238 6.47625L11 5.5Z" fill="currentColor"/></svg>
                        </span>
                      </summary>
                    {%- endif -%}
                    <div class="hdt-widget-content">
                      <div class="hdt-raw-html hdt-rte--list" id="b_{{ block.id }}">{{ block.settings.html_content }}</div>
                    </div>
                  </details>
                  </hdt-accordion>
                {%- when 'social' -%}
                  {{ 'hdt-icon-social.css' | asset_url | stylesheet_tag }}
                  <hdt-accordion class="hdt-contents">
                  <details class="hdt-widget hdt-widget-social" open {{ block.shopify_attributes }}>
                    {%- if block.settings.widget_heading != blank -%}
                      <summary class="hdt-widget-title hdt-flex hdt-align-center hdt-justify-between hdt-text-xl lg:hdt-text-2xl hdt-font-normal">
                        <span>{{ block.settings.widget_heading }}</span>
                        <span accordion-icon>
                          <svg xmlns="http://www.w3.org/2000/svg" width="11" height="7" viewBox="0 0 11 7" fill="none"><path d="M11 5.5L5.5 6.55868e-08L-6.55868e-08 5.5L0.97625 6.47625L5.5 1.9525L10.0238 6.47625L11 5.5Z" fill="currentColor"/></svg>
                        </span>
                      </summary>
                    {%- endif -%}
                    <div class="hdt-widget-content">
                      {%- render 'social_sharing', socials_style: block.settings.socials_style, socials_size: block.settings.socials_size -%}
                    </div>
                  </details>
                  </hdt-accordion>
                {%- when 'filter' -%}
                  {%- liquid
                    assign results = collection
                    assign color_list = 'products.theme_settings.color' | t | downcase | strip | replace: ' ,', ',' | replace: ', ', ',' | split: ',' | uniq
                    assign color_white = 'products.theme_settings.color_white' | t | downcase | strip
                    assign sort_by = results.sort_by | default: results.default_sort_by
                    if results.url
                      assign results_url = results.url
                      assign action = results_url
                    else
                      assign action = routes.search_url
                      assign terms = results.terms | escape
                      assign results_url = '?q=' | append: terms | append: '&options%5Bprefix%5D=last&sort_by=' | append: sort_by
                    endif
                  -%}

                  {{ 'facets.css' | asset_url | stylesheet_tag }}
                  <hdt-facet-filters-form section-id="{{ section.id }}" render-on-change>
                    {% if request.design_mode %}
                      {%- assign suffix = '?view=' | append: template.suffix -%}
                      {%- assign action = action | append: suffix -%}
                    {% endif %}
                    <form class="hdt-filter" id="hdt-facet-filters-form-sidebar" method="GET" action="{{ action }}" section-id="{{ section.id }}"render-on-change>
                      <input type="hidden" name="page" value="">
                      {%- for filter in results.filters -%}
                        <hdt-accordion class="hdt-contents">
                          <details class="hdt-filter-group" open>
                            <summary class="hdt-filter-group-summary"{% if filter.type != 'price_range' %} aria-label="{{ filter.label }} ({{ 'products.facets.filters_selected.one' | t: count: filter.active_values.size }})"{% endif %}>
                              <div class="hdt-facet-title hdt-text-lg lg:hdt-text-2xl hdt-font-normal hdt-s-text hdt-flex">
                                <span class="hdt-inline-flex hdt-facet-title_group">
                                  <span>{{ filter.label }}</span>
                                  {% if filter.active_values.size > 0 %}
                                    <span class="hdt-hidden">
                                      ({{ filter.active_values.size }})
                                    </span>
                                  {% endif %}
                                </span>
                                <span>
                                  <svg class="hdt-facet-title_icon" xmlns="http://www.w3.org/2000/svg" width="11" height="7" viewBox="0 0 11 7" fill="currentColor">
                                    <path d="M11 5.5L5.5 6.55868e-08L-6.55868e-08 5.5L0.97625 6.47625L5.5 1.9525L10.0238 6.47625L11 5.5Z"/>
                                  </svg>
                                </span>
                              </div>
                            </summary>
                            <div class="hdt-filter-group__display">
                              {%- if filter.type != 'price_range' -%}
                              <div class="hdt-filter-group__header hdt-hidden">
                                <span class="hdt-filter-group__header-selected hdt-base hdt-font-normal hdt-s-text">{{ 'products.facets.filters_selected' | t: count: filter.active_values.size }}</span>
                                {%- if filter.active_values.size > 0 -%}
                                  <wrapp-hdt-facet-url><a href="{{ filter.url_to_remove }}" class="hdt-filter-group__header-reset">{{ 'products.facets.reset' | t }}</a></wrapp-hdt-facet-url>
                                {%- endif -%}
                              </div>
                              {%- endif -%}
                              {%- case filter.type -%}
                                {%- when 'boolean' -%}
                                  <ul class="hdt-filter-group__list type_boolean">
                                    {%- for filter_value in filter.values -%}
                                      <li class="hdt-filter-group__list-item">
                                        <input class="" type="checkbox" name="{{ filter_value.param_name }}" value="{{ filter_value.value }}" id="Filter-sidebar-{{ filter.param_name }}-{{ forloop.index }}"{% if filter_value.active %} checked{% endif %}{% if filter_value.count == 0 and filter_value.active == false %} disabled{% endif %}>
                                        <label for="Filter-sidebar-{{ filter.param_name }}-{{ forloop.index }}">{{ filter_value.label }}</label>
                                      </li>
                                    {%- endfor -%}
                                  </ul>
                                  {% comment %} <div class="hdt-filter-group__submit"><input type="submit" value="{{ 'products.facets.apply' | t }}"></div> {% endcomment %}

                                {%- comment %} {%- when 'boolean' or 'list' -%} {% endcomment -%}
                                {%- when 'list' -%}
                                  {%- assign label_downcase = filter.label | downcase -%}
                                  {%- if color_list contains label_downcase -%}

                                    {%- liquid
                                      assign colors = shop.metafields.ecomrise.colors.value
                                      assign color_values = shop.metaobjects.theme_swatch.values | first
                                      assign colors2 = color_values.colors.value
                                    -%}

                                    <ul class="hdt-filter-group__list hdt-filter-group__list--color">
                                      {%- for filter_value in filter.values -%}

                                        {%- liquid
                                        assign color_downcase = filter_value.label | downcase
                                        assign color_handle = filter_value.label | handle
                                        assign color_obj = colors[color_handle] | default: colors2[color_handle]
                                        assign color_style_inline = ''
                                        assign image = images[color_obj.imageName]
                                        if image != blank
                                          assign color_image = image | image_url: width: 80
                                          assign color_image_style = '--hdt-bg-image: url(' | append: color_image | append: ');'
                                        else
                                          assign color_image_style = nil
                                          assign color_style_inline = color_obj.colorsCSS
                                        endif -%}
                                        <li class="hdt-filter-group__list-item hdt-relative hdt-align-center">
                                          <input class="sr-only" type="checkbox" name="{{ filter_value.param_name }}" value="{{ filter_value.value }}" id="Filter-sidebar-{{ filter.param_name }}-{{ forloop.index }}"{% if filter_value.active %} checked{% endif %}{% if filter_value.count == 0 and filter_value.active == false %} disabled{% endif %}>
                                          <label for="Filter-sidebar-{{ filter.param_name }}-{{ forloop.index }}">
                                            <span class="hdt-relative">
                                              <span class="hdt-facets-checkbox-color hdt-filter-group__color{% if color_white == color_downcase %} hdt-bg-color-white{% endif %}"{% if color_image_style or color_style_inline.size > 0 %} style="{{ color_image_style }}{{ color_style_inline }}"{% endif %}>
                                                <span class="sr-only">{{ filter_value.label }}</span>
                                              </span>
                                              <span class="hdt-facets-color-icon hdt-full-width-link hdt-flex hdt-align-center hdt-justify-center hdt-pe-none">
                                                <svg focusable="false" width="24" height="24" viewBox="0 0 24 24" role="presentation"><path fill="currentColor" d="M9 20l-7-7 3-3 4 4L19 4l3 3z"></path></svg>
                                              </span>
                                            </span>
                                            <span class="hdt-base hdt-font-normal hdt-filter-group__color-label"><span>{{ filter_value.label }}</span> <span> ({{ filter_value.count }})</span></span>
                                          </label>
                                        </li>
                                      {%- endfor -%}
                                    </ul>
                                  {%- else -%}
                                    <ul class="hdt-filter-group__list">
                                      {%- for filter_value in filter.values -%}
                                        <li class="hdt-filter-group__list-item">
                                          <input class="sr-only" type="checkbox" name="{{ filter_value.param_name }}" value="{{ filter_value.value }}" id="Filter-sidebar-{{ filter.param_name }}-{{ forloop.index }}"{% if filter_value.active %} checked{% endif %}{% if filter_value.count == 0 and filter_value.active == false %} disabled{% endif %}>
                                          <label for="Filter-sidebar-{{ filter.param_name }}-{{ forloop.index }}">
                                            <span class="hdt-facets-checkbox" >
                                              <svg focusable="false" width="24" height="24" viewBox="0 0 24 24" role="presentation"><path fill="currentColor" d="M9 20l-7-7 3-3 4 4L19 4l3 3z"></path></svg>
                                            </span>
                                            <span class="hdt-facets-label hdt-base hdt-normal">
                                              <span>{{ filter_value.label }}</span> <span> ({{ filter_value.count }})</span>
                                            </span>
                                          </label>
                                        </li>
                                      {%- endfor -%}
                                    </ul>
                                  {%- endif -%}
                                  {%- comment %}<div class="hdt-filter-group__submit"><input type="submit" value="{{ 'products.facets.apply' | t }}"></div>{% endcomment -%}

                                {%- when 'price_range' -%}
                                  {%- liquid
                                    assign min_value = filter.min_value.value | default: 0 | divided_by: 100.0
                                    assign max_value = filter.max_value.value | default: filter.range_max | divided_by: 100.0
                                    assign step_value = 1
                                    assign min_value_ceil = min_value | ceil
                                    assign max_value_ceil = max_value | ceil
                                    assign range_max_ceil = filter.range_max | divided_by: 100.0 | ceil
                                    assign min_progress = min_value | divided_by: range_max_ceil | times: 100.0
                                    assign max_progress = max_value | divided_by: range_max_ceil | times: 100.0
                                    if section.settings.price_behavior == 'click'
                                      assign behavior = 'disable-change'
                                    else
                                      assign behavior = ''
                                    endif
                                  -%}
                                  <div class="hdt-price-range_wrap">
                                    <hdt-price-range class="hdt-price-range hdt-filter-group__price-range" style="--min-progress: {{ min_progress }}%; --max-progress: {{ 100.0 | minus: max_progress }}%">
                                      <div class="hdt-filter-group__range-slider">
                                        <div class="hdt-filter-group__range-progress">
                                          <div class="hdt-progress"></div>
                                        </div>
                                        <div class="hdt-filter-group__range-price hdt-relative">
                                          <input {{ behavior }} aria-label="{{ 'products.facets.from_price' | t | escape }}" type="range" step="{{ step_value }}" min="0" max="{{ range_max_ceil }}" value="{% if filter.min_value.value %}{{ min_value_ceil }}{% else %}0{% endif %}">
                                          <input {{ behavior }} aria-label="{{ 'products.facets.to_price' | t | escape }}" type="range" step="{{ step_value }}" min="0" max="{{ range_max_ceil }}" value="{% if filter.max_value.value %}{{ max_value_ceil }}{% else %}{{ range_max_ceil }}{% endif %}">
                                        </div>
                                      </div>
                                      <div class="hdt-filter-group__input-price-wrap hdt-flex hdt-justify-between hdt-align-center">
                                        <div class="hdt-filter-group__input-price hdt-flex hdt-align-center">
                                          <span>{{ 'products.facets.price' | t }}</span>
                                          <div class="hdt-filter-group__price-range-from hdt-flex hdt-align-center">
                                            <span>{{ cart.currency.symbol }}</span>
                                            <input {{ behavior }} aria-label="{{ 'products.facets.from_price' | t | escape }}" inputmode="numeric" name="{{ filter.min_value.param_name }}" type="number" step="{{ step_value }}" min="0" max="{{ range_max_ceil }}" {% if filter.min_value.value %}value="{{ min_value_ceil }}"{% else %}placeholder="0"{% endif %}>
                                          </div>
                                          <span class="hdt-inline-block">-</span>
                                          <div class="hdt-filter-group__price-range-to hdt-flex hdt-align-center">
                                            <span>{{ cart.currency.symbol }}</span>
                                            <input {{ behavior }} aria-label="{{ 'products.facets.to_price' | t | escape }}" inputmode="numeric" name="{{ filter.max_value.param_name }}" type="number" step="{{ step_value }}" min="0" max="{{ range_max_ceil }}" {% if filter.max_value.value %}value="{{ max_value_ceil }}"{% else %}placeholder="{{ range_max_ceil }}"{% endif %}>
                                          </div>
                                        </div>
                                        {%- if section.settings.price_behavior == 'click' %}<wrapp-hdt-btn-submit-filter><button class="hdt-btn hdt-btn-filter-price" type="submit">{{ 'products.facets.filter_button' | t | escape }}</button></wrapp-hdt-btn-submit-filter>{% endif -%}
                                      </div>
                                    </hdt-price-range>
                                    {%- comment %}<div class="hdt-filter-group__submit"><input type="submit" value="{{ 'products.facets.apply' | t }}"></div>{% endcomment -%}
                                  </div>

                              {%- endcase -%}
                            </div>
                          </details>
                        </hdt-accordion>
                      {%- endfor -%}
                    </form>
                  </hdt-facet-filters-form>
              {%- endcase -%}
            {% endfor %}
          {% endcapture %}
          {%- if section.settings.sidebar_drawer == false -%}
            <aside class="hdt-main-sidebar hdt-col hdt-sidebar-{{ section.settings.sidebar_size }} {% if section.settings.sidebar_position == 'left' %} lg:hdt-order-first hdt-left-0 hdt-main-sidebar-left{% else %}hdt-right-0 hdt-main-sidebar-right{% endif %}"  {%- if section.settings.colors_by_section_sb %} color-scheme="{{ section.settings.color_scheme_sb }}" style="--background-opacity: {{ section.settings.background_opacity_sb }};"{%- endif %}>
              <div class="hdt-main-sidebar-inner hdt-current-scrollbar">
                {{ sidebar_content }}
              </div>
            </aside>
          {%- else -%}
            
            <button aria-controls="hdt-shop-sidebar-drawer" class="{% unless section.settings.sidebar_drawer %}lg:hdt-hidden{% endunless %} hdt-flex hdt-align-center hdt-sidebar-btn hdt-fixed {% if section.settings.sidebar_position == 'left' %} hdt-left-0{% else %}hdt-right-0{% endif %}">
